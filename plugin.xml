<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-kount-ex" version="0.6.2" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>KountExample</name>
	<description>Kount integration plugin</description>
	<license>MIT</license>
	<js-module name="KountExample" src="www/KountExample.js">
		<clobbers target="KountExample" />
	</js-module>
	<engines>
	    <engine name="cordova-android" version=">=4.0.0" /> <!-- This main branch of OneSignal depends on gradle and maven. -->
	    <engine name="apple-xcode" version=">=6.2.0" />
	    <engine name="apple-ios" version=">=7.0.0" />
    </engines>
	<platform name="android">
		<config-file target="res/xml/config.xml" parent="/*">
			<feature name="KountExample">
				<param name="android-package" value="cordova-plugin-kount-ex.KountExample"/>
			</feature>
		</config-file>
		<source-file src="src/android/KountExample.java" target="src/cordova-plugin-kount-ex" />
	</platform>
	<platform name="ios">
	</platform>
</plugin>