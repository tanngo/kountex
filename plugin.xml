<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-kount-ex" version="0.3.9" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>KountExample</name>
	<description>Kount integration plugin</description>
	<license>MIT</license>
	<js-module name="KountExample" src="www/KountExample.js">
		<clobbers target="cordova.plugins.KountExample" />
	</js-module>
	<dependency id="cordova-plugin-device" />
	<dependency id="cordova-plugin-console" />
	<config-file parent="/*" target="AndroidManifest.xml">		
			<feature name="KountExample">
			<param name="android-package" value="cordova-plugin-kount-ex.KountExample" />
			</feature>	
	</config-file>
	<source-file src="src/android/kountex/KountExample.java" target-dir="src/kountex/KountExample" />
	<source-file src="src/android/kountex/lib/kount-data-collector-3.1.jar" target-dir="src/kountex/KountExample" />
	<source-file src="src/android/kountex/lib/ClassLibEx.jar" target-dir="src/kountex/lib/AlertEx" />
</plugin>